                android:layout_height="200dp"
                android:layout_marginStart="24dp"
                android:layout_marginTop="1dp"
                android:layout_marginBottom="1dp"
                app:cardCornerRadius="10dp"
                app:layout_constraintBottom_toBottomOf="parent"
